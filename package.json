{
  "name": "thought-system",
  "version": "1.0.0",
  "description": "Personal thought management system with OpenClaw integration",
  "main": "src/index.js",
  "bin": {
    "thought-system": "./bin/thought-system"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "node -e \"const { Database } = require('./src/database'); new Database().migrate()\"",
    "job:hourly": "node src/cron/jobs.js hourly",
    "job:daily": "node src/cron/jobs.js daily",
    "check:updates": "node -e \"const { Updater } = require('./src/updater'); new Updater().checkForUpdates().then(r => console.log(r))\"",
    "test": "jest",
    "install-cron": "bash scripts/setup-cron.sh"
  },
  "keywords": [
    "thought-system",
    "openclaw",
    "notes",
    "cli",
    "automation"
  ],
  "author": "Tyler Evans",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
